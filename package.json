{
  "name": "e-recruiting",
  "version": "1.0.1",
  "adonis-version": "4.1.0",
  "description": "Recruiting questionnaire constructor power by React and AdonisJS.",
  "main": "server.js",
  "scripts": {
    "start": "adonis serve -p",
    "postinstall": "cd client && yarn install",
    "dev": "adonis serve --dev",
    "client": "cd client && node scripts/start.js",
    "build": "rm -rf public && cd client && node scripts/build.js && cp -rfv build/ ../public",
    "test": "node ace test",
    "migrate": "adonis migration:run --seed --force"
  },
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cli": "^4.0.12",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/session": "^1.0.27",
    "@adonisjs/shield": "^1.0.8",
    "@adonisjs/validator": "^5.0.6",
    "@types/knex": "^0.16.1",
    "chance": "^1.1.4",
    "faker": "^4.1.0",
    "lodash": "^4.17.15",
    "mysql": "^2.18.1",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {},
  "autoload": {
    "App": "./app"
  }
}
